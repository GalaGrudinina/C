#include <stdio.h>
#include <string.h>
 
int main(){
    int space=0;
    int newstr=0;
    int i=0;
    char str[100];
    printf("Enter the sentense:");
    gets(str);
    while(str[i]!= '\0')
    {
        if (str[i]==' ')
        {
            space++;
        }
        if(str[i]=='\n')
        {
            newstr++;
        }
                i++;
    }
 printf("%d %d \n",space, newstr);
 return 0;
}
