#include<stdio.h>
int main()
{
 char *month[12]={"January","February","March","April","May","June","July","August","September","October","November","December"};
 int m,d;
 printf("Enter a Month Number:");
 scanf("%d",&m);
 printf("Enter the day:");
 scanf("%d",&d);
 printf("Today is %d %s ",d, month[m-1]);
    return 0;
}
